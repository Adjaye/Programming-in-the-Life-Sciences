PREFIX dbvoc: <http://bio2rdf.org/drugbank_vocabulary:> 
PREFIX dcterms: <http://purl.org/dc/terms/> 

select ?target ?title ?titleCode ?function 

where {

   <http://bio2rdf.org/drugbank:DB00900> dbvoc:target ?target . 
   ?target dcterms:title ?title .
   ?target rdfs:label ?titleCode .
   ?target dbvoc:go-function ?function .

} LIMIT 100
