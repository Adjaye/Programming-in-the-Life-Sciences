PREFIX dbvoc: <http://bio2rdf.org/drugbank_vocabulary:>
PREFIX dcterms: <http://purl.org/dc/terms/>

select ?drug where {

?drug dbvoc:indication <http://bio2rdf.org/drugbank_resource:79431ac7eb5fe1dc2042a4233ef058e3>. 
?drug dbvoc:target ?target . 
?target dbvoc:x-kegg ?keggid. 
?keggid (keggvoc):pathway ?pathwayid

} LIMIT 100
